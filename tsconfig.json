{
  "compilerOptions":
  {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/main/ts",
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "preserveConstEnums": false,
    "removeComments": true,
    "sourceMap": false,
    "strict": true,
    "target": "es2020",
  }
}

{
  "name": "@snaplib/compare",
  "version": "0.1.4",
  "description": "Compare 2 JavaScript objects to each other.",
  "private": true,
  "main": "./lib/compare.js",
  "types": "./types/compare.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "homepage": "https://www.github.com/SnapLib/typescript-compare/tree/release",
  "author": {
    "name": "Snap",
    "email": "snap@snaplib.org",
    "url": "https://www.github.com/SnapLib"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/SnapLib/typescript-compare/tree/release"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "snaplib",
    "javascript",
    "object",
    "compare",
    "utility"
  ],
  "scripts": {
    "build-dist": "tsc -p ./tsconfig.json",
    "build-tsdoc": "typedoc --options ./typedoc.json",
    "lint": "eslint --quiet --ext ts ./src/**/*.ts",
    "lint-strict": "eslint --ext ts ./src/**/*.ts",
    "test": "env TS_NODE_PROJECT='./src/test/tsconfig.json' mocha --config ./src/test/.mocharc.cjs",
    "test-report": "env TS_NODE_PROJECT='./src/test/tsconfig.json' mocha --config ./src/test/report.mocharc.cjs",
    "tsnode": "ts-node"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^9.0.0",
    "mochawesome": "^6.2.2",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "optionalDependencies": {
    "@types/node": "^15.6.1"
  }
}

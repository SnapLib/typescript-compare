{
  "name": "object-compare",
  "version": "0.0.1",
  "description": "Compare 2 JavaScript objects.",
  "main": "objectCompare.js",
  "author": {
    "name": "Snap",
    "email": "snap@snaplib.org"
  },
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "keywords": [
    "snaplib",
    "javascript",
    "object",
    "compare",
    "utility"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "types": "./types",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "build-tsdoc": "typedoc --options ./typedoc.json",
    "lint": "eslint --quiet --ext ts ./src",
    "lint-strict": "eslint --ext ts ./src",
    "test": "env TS_NODE_PROJECT='./src/test/tsconfig.json' mocha --config ./.mocharc.js",
    "test-report": "env TS_NODE_PROJECT='./src/test/tsconfig.json' mocha --config ./report.mocharc.js"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "mocha": "^8.4.0",
    "mochawesome": "^6.2.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
